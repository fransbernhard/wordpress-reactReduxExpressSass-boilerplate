{
  "name": "mimist",
  "version": "1.0.0",
  "description": "Mimis try to better herself",
  "author": {
    "name": "Mimi Lundberg",
    "email": "mimilundberg@fransbernhard",
    "url": "http://fransbernhard.com"
  },
