  "engines": {
    "node": "8.9.0"
  },
  "scripts": {
    "dev": "rimraf build-dev && NODE_ENV=development webpack --config=webpack/config.dev.js && node ./public/server/server.js",
    "prebuild": "rimraf build",
    "build": "yarn build:client && yarn build:server",
    "build:client": "NODE_ENV=production webpack -p --progress --config=webpack/config.prod.js",
    "build:server": "copyfiles -a -u 1 public/server/**/**/* build"
  },
  "devDependencies": {
    "babel-cli": "*",
    "babel-core": "*",
    "babel-loader": "*",
    "babel-preset-env": "*",
