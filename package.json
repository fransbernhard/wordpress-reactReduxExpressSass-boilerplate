    "cross-env": "*",
    "css-loader": "*",
    "extract-text-webpack-plugin": "*",
    "file-loader": "^1.1.10",
    "html-loader": "*",
    "html-webpack-harddisk-plugin": "^0.1.0",
    "html-webpack-plugin": "*",
    "node-sass": "*",
    "open": "^0.0.5",
    "postcss-loader": "*",
    "rimraf": "*",
    "sass-loader": "*",
    "style-loader": "*",
    "uglifyjs-webpack-plugin": "*",
    "url-loader": "^0.6.2",
    "webpack": "3.10.0",
    "webpack-dev-middleware": "*",
    "webpack-hot-middleware": "*",
    "webpack-merge": "*"
  },
  "dependencies": {
    "body-parser": "*",
    "compression": "*",
    "express": "*",
    "prop-types": "*",
